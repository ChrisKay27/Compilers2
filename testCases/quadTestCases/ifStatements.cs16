int main(void){
    int x;
    x := 0;
    loop {
        x := x + 1;
        if( x > 5 )
            if( x > 7 ) {
                writeint(10);
                if ( x >= 10) exit;
            }
            else writeint(7);
        else 
            if (x > 3) writeint(5);
            else writeint(3);
        continue;
    } end;
    return x;
}