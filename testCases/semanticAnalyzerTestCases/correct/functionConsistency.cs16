void fred(void){
    writebool(true);
}

void foo(int x, ref bool b, int ab, ref bool bar){

    if( x = ab ) b := true;
    else bar := true;

}

int main(void){
    bool b, bar, y;
    int x;
    int a[5];
    int b;

    x := 2;
    y := true;
    b := 2;

    foo(x+x, not y, a[2], bar);
    fred();
    writebool(b);
    writebool(bar);
    return -1;
}